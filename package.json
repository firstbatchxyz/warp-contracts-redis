{
  "name": "warp-contracts-redis",
  "version": "0.1.4",
  "description": "Redis implementation for Warp Contracts",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "license": "MIT",
  "author": "FirstBatch Team <dev@firstbatch.xyz>",
  "contributors": [
    "Faruk Can Ã–zkan <farukcan@firstbatch.xyz>",
    "Erhan Tezcan <erhan@firstbatch.xyz>"
  ],
  "exports": {
    ".": "./lib/cjs/index.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.5"
  },
  "keywords": [
    "key-value store",
    "storage",
    "database",
    "blockchain",
    "arweave",
    "smartweave",
    "warp-contracts",
    "redis",
    "cache"
  ],
  "scripts": {
    "build:cjs": "tsc -b tsconfig.json",
    "build:types": "tsc -b tsconfig.types.json",
    "build": "yarn run clean && yarn build:cjs && yarn build:types",
    "format": "prettier --write 'src/**/*.ts'",
    "clean": "rimraf ./lib",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepublishOnly": "yarn lint:fix",
    "preversion": "yarn lint && yarn build",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "yalc:publish": "yarn build && yalc publish --push",
    "test": "jest --detectOpenHandles"
  },
  "files": [
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@redis/client": "^1.5.6",
    "warp-contracts": "^1.4.2"
  }
}
